<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.scheduling_activity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name=".RegisterActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.labels.MeetingActivityLabels" />
        <activity android:name=".ui.labels.JabatanActivityLabels" />
        <activity android:name=".ui.labels.StatusActivityLabels" />
        <activity android:name=".LoginActivity"/>
        <activity android:name=".ui.detail.DetailKegiatan" />

        <receiver android:name=".ui.alarm.service.AlarmReceiver" />
    </application>

</manifest>